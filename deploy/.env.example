# SwitchBot API（必須）
SWITCHBOT_TOKEN=your_switchbot_token
SWITCHBOT_SECRET=your_switchbot_secret

# InfluxDB（必須）
# openssl rand -hex 32 で生成推奨
INFLUXDB_TOKEN=your_influxdb_admin_token

# InfluxDB 管理者パスワード（オプション）
# INFLUXDB_PASSWORD=adminpassword

# Grafana 認証情報（オプション）
# GRAFANA_USER=admin
# GRAFANA_PASSWORD=admin

# イメージバージョン（オプション）
# VERSION=latest

# データ収集間隔（オプション、ミリ秒）
# COLLECT_INTERVAL_MS=300000
